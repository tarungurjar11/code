//https://practice.geeksforgeeks.org/problems/last-index-of-1/0



#include <iostream>
using namespace std;
int find(string s,char ch)
{
    int x=-1;
    for(int i=0;i<s.length();i++)
    {
        if(s[i]==ch)
            x=i;
        
    }
    return x;
}
int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    string s;
	    cin>>s;
	    int x=find(s,'1');
	    cout<<x<<endl;
	}
	return 0;
}
